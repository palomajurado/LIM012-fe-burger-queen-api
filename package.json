{
  "name": "burger-queen-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "dev": "nodemon src/index.js --exec babel-node",
    "start": "node src/index.js",
    "pretest": "eslint . --fix",
    "test:e2e": "jest --verbose --testPathPattern e2e --globalSetup ./e2e/globalSetup --globalTeardown ./e2e/globalTeardown --runInBand",
    "test:unit": "jest --verbose --testPathIgnorePatterns mock-express e2e",
    "test": "npm run test:e2e && npm run test:unit",
    "lint": "eslint .",
    "docs:generate": "jsdoc -c jsdoc.conf --readme README-docs.md",
    "docs:deploy": "rm -rf docs && npm run docs:generate && gh-pages -d docs"
  },
  "dependencies": {
    "@babel/polyfill": "7.10.1",
    "@shelf/jest-mongodb": "^1.2.2",
    "@std/esm": "^0.26.0",
    "babel-node": "0.0.1-security",
    "bcrypt": "5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.19.0",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "mongodb": "3.5.9",
    "mongodb-memory-server": "^6.6.1",
    "mongoose": "5.9.20",
    "mongoose-paginate-v2": "^1.3.9",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0",
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.10.3",
    "@babel/core": "7.10.3",
    "@babel/node": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "babel-jest": "^26.1.0",
    "docdash": "1.1.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jasmine": "^4.1.1",
    "gh-pages": "2.1.1",
    "jest": "24.9.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdoc": "3.6.3",
    "jsdoc-http-plugin": "0.3.1",
    "node-fetch": "2.6.0",
    "nodemon": "2.0.4",
    "tree-kill": "^1.2.2"
  }
}
